{
    "name": "hitster-client",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "yaml-sort": "run-script-os",
        "yaml-sort:win32": "for %f in (src/locale/*.yml) do npx yaml-sort --indent 4 -i \"src/locale/%f\"",
        "yaml-sort:default": "for file in src/locale/*.yml; do npx yaml-sort --indent 4 -i \"src/locale/$file\"; done",
        "format": "npm run yaml-sort && npx prettier --write .",
        "dev": "vite",
        "build": "tsc && vite build",
        "build-dev": "tsc && vite build -m=development --sourcemap true --minify false",
        "lint": "eslint ./src -c eslint.config.mjs --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "version": "node -e \"import('@corteks/gitversion').then(({default: _}) => _.default().then((gv) => console.log(!gv.COMMITS_SINCE_TAG ? gv.LAST_TAG_NAME : gv.LAST_TAG_NAME + '+' + gv.COMMITS_SINCE_TAG + '-' + gv.CURRENT_COMMIT_SHORT_ID)))\""
    },
    "dependencies": {
        "@dnd-kit/sortable": "^10.0.0",
        "@dr.pogodin/react-helmet": "^3.0.1",
        "@lomray/event-manager": "^2.0.2",
        "@rwh/keystrokes": "^1.5.6",
        "@uidotdev/usehooks": "^2.4.1",
        "@ungap/structured-clone": "^1.3.0",
        "boolify-string": "^2.0.2",
        "bootstrap": "^5.3.3",
        "classnames": "^2.5.1",
        "deep-copy": "^1.4.2",
        "detect-browser": "^5.3.0",
        "howler": "^2.2.4",
        "i18next": "^25.2.1",
        "i18next-browser-languagedetector": "^8.0.2",
        "js-convert-case": "^4.2.0",
        "natsort": "^2.0.3",
        "pick-random": "^4.0.0",
        "query-string": "^9.2.2",
        "react": "^19.1.0",
        "react-bootstrap": "^2.10.9",
        "react-cookie": "^8.0.1",
        "react-dom": "^19.1.0",
        "react-i18next": "^16.0.0",
        "react-icons": "^5.5.0",
        "react-router": "^7.5.0",
        "react-toastify": "^11.0.5",
        "react-youtube": "^10.1.0",
        "slugify": "^1.6.6",
        "title-case": "^4.3.1",
        "use-effect-event": "^1.0.2",
        "use-immer": "^0.11.0",
        "use-prefers-color-scheme": "^1.1.3",
        "zod": "^4.0.17"
    },
    "devDependencies": {
        "@corteks/gitversion": "^0.3.4",
        "@eslint/compat": "^1.2.4",
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.16.0",
        "@modyfi/vite-plugin-yaml": "^1.1.0",
        "@types/boolify-string": "^0.0.32",
        "@types/howler": "^2.2.11",
        "@types/react": "^19.1.0",
        "@types/react-bootstrap": "^1.0.1",
        "@types/react-dom": "^19.1.2",
        "@types/react-router-bootstrap": "^0.26.6",
        "@types/ungap__structured-clone": "^1.2.0",
        "@typescript-eslint/eslint-plugin": "^8.18.0",
        "@typescript-eslint/parser": "^8.18.0",
        "@vitejs/plugin-react-swc": "^4.1.0",
        "cli-glob": "^0.1.0",
        "eslint": "^9.16.0",
        "eslint-plugin-react-hooks": "^7.0.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "globals": "^16.0.0",
        "prettier": "^3.2.5",
        "prettier-plugin-organize-imports": "^4.1.0",
        "run-script-os": "^1.1.6",
        "typescript": "^5.2.2",
        "vite": "npm:rolldown-vite@latest",
        "vite-plugin-checker": "^0.11.0",
        "vite-plugin-filter-replace": "^0.1.13",
        "vite-tsconfig-paths": "^5.1.4",
        "yaml-sort": "^2.1.0"
    },
    "overrides": {
        "@modyfi/vite-plugin-yaml": {
            "vite": "$vite"
        }
    }
}
